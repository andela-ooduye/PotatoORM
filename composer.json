{
  "name": "ooduye/potato-orm",
  "description": "A simple agnostic PHP ORM",
  "keywords": [
    "ORM",
    "PHP",
    "ooduye"
  ],
  "homepage": "https://github.com/andela-ooduye/PotatoORM",
  "license": "MIT",
  "authors": [
    {
      "name": "Oduye Oluwayemisi",
      "email": "oduye.oluwayemisi@andela.com",
      "role": "Developer",
      "homepage": "http://oluwayemisioduye.com"
    }
  ],
  "require": {
    "php" : ">=5.3.0",
    "vlucas/phpdotenv": "^2.0",
    "Mockery/Mockery": ">=0.7.2"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Yemisi\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "Yemisi\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
